package com.esiag.webservice;

import java.net.URL;

import javax.xml.namespace.QName;
import javax.xml.ws.Service;



public class Main {

	
	public Main()
	{
		try {
			 URL url=new URL("http://localhost:8888/WebServiceServeur?wsdl");
				QName qname=new QName("http://webservice.esiag.com/","ServiceService");
				Service service=Service.create(url, qname);
				client.IWebService proxy=service.getPort(IWebService.class);
				System.out.println(proxy.message("Driss"));
		} catch (Exception e) {
			// TODO: handle exception
		}
		
	}
	public static void main(String[] args) {
		new Main();

	}

}
