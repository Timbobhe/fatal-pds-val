	<element name="Message">
		<complexType>
			<sequence>
				<element name="Message" type="tns:Messageinfos" maxOccurs="1"></element>
			</sequence>
		</complexType>
	</element>
	
<!-- Message Structure -->

	<!-- Complexes types Declaration -->
<complexType name="Messageinfos">
		
	    <attribute name="Source" type="string" use="required"></attribute>
		<attribute name="codeTypeEvenement" type="int" use="required"></attribute>
		<attribute name="position" type="string" use="required"></attribute>
		<attribute name="Criticite" type="int" use="required"></attribute>
</complexType>
</schema>